{
    "targets": {
      "debug": {
        "outFile": "build/translator.wasm",
        "textFile": "build/translator.wat",
        "sourceMap": true,
        "debug": true
      },
      "release": {
        "outFile": "build/translator.wasm",
        "textFile": "build/translator.wat",
        "sourceMap": true,
        "optimizeLevel": 3,
        "shrinkLevel": 0,
        "converge": false,
        "noAssert": false
      }
    },
    "options": {
      "bindings": "esm",
      "importMemory": true,
      "initialMemory": 16,
      "maximumMemory": 32,
      "noExportMemory": false,
      "runtime": "incremental"
    }
  }